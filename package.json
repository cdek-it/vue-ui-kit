{
  "name": "@cdek-it/vue-ui-kit",
  "version": "3.0.0-beta.ds226",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-ui-kit.umd.js",
  "module": "./dist/vue-ui-kit.es.js",
  "types": "./dist/vue-ui-kit.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-ui-kit.es.js",
      "require": "./dist/vue-ui-kit.umd.js"
    },
    "./scss": {
      "import": "./dist/scss/index.scss"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-p type-check build-only",
    "test": "vitest --environment jsdom --root src/",
    "test:cli": "vitest run --environment jsdom --root src/",
    "test:ui": "vitest --environment jsdom --root src/ --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:cli": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings 0",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "STORYBOOK_BASE_HREF=vue-ui-kit build-storybook",
    "gen:component": "bash ./scripts/generate-component.sh",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.12",
    "@primevue/auto-import-resolver": "4.3.6",
    "@primevue/themes": "4.3.6",
    "@tabler/icons-webfont": "^3.22.0",
    "@vee-validate/rules": "^4.7.3",
    "lodash": "^4.17.21",
    "maska": "^2.1.11",
    "primevue": "^4.5.3",
    "tailwindcss-primeui": "0.6.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.15",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.2.0",
    "@vitest/ui": "^0.27.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^8.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^9.1.7",
    "jsdom": "^20.0.3",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~4.7.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-static-copy": "^0.13.0",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.6.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdek-it/vue-ui-kit.git"
  }
}